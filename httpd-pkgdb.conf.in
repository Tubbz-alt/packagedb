Alias /pkgdb/static @DATADIR@/fedora-packagedb/pkgdb/static

WSGIDaemonProcess pkgdb user=apache group=apache maximum-requests=1000 display-name=pkgdb processes=4 threads=1
WSGISocketPrefix run/wsgi
WSGIRestrictStdout On
WSGIRestrictSignal Off
WSGIPythonOptimize 1

WSGIScriptAlias /pkgdb @SBINDIR@/pkgdb.wsgi/pkgdb

<Location /pkgdb>
    WSGIProcessGroup pkgdb
    Order deny,allow
    Allow from all
</Location>
